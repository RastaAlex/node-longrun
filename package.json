{
  "name": "longrun",
  "version": "2.4.1",
  "description": "Run set of commands in group of directories",
  "main": "lib/longrun.js",
  "bin": {
    "longrun": "bin/longrun.js",
    "longrun-legacy": "legacy/bin/longrun.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "test": "tape 'test/**/*.js'",
    "watch": "nodemon --watch lib --watch test --exec",
    "watch:test": "watch -- npm test",
    "lint:jscs": "jscs --esnext lib test",
    "lint:jshint": "jshint lib test",
    "lint:eslint": "eslint lib test",
    "lint": "redrun lint:*",
    "jscs-fix": "npm run jscs --fix",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "wisdom": "npm run build",
    "wisdom:done": "npm i -g",
    "6to5:lib": "babel lib -d legacy/lib",
    "6to5:bin": "babel bin -d legacy/bin",
    "build": "redrun 6to5:* legacy:*",
    "legacy:index": "echo \"module.exports = require('./lib/longrun');\" > legacy/index.js",
    "legacy:package": "echo \"module.exports = require('../package');\" > legacy/package.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-longrun.git"
  },
  "keywords": [
    "run",
    "runny",
    "directory",
    "shell",
    "execute",
    "spawn"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/node-longrun/issues"
  },
  "homepage": "https://github.com/coderaiser/node-longrun",
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.6",
    "eslint": "^3.0.1",
    "jscs": "^3.0.1",
    "jshint": "^2.8.0",
    "nodemon": "^1.9.2",
    "nyc": "^7.0.0",
    "redrun": "^5.0.0",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "apart": "^1.0.1",
    "async": "^2.0.0-rc.4",
    "currify": "^1.0.0",
    "readjson": "^1.1.3",
    "runny": "^1.4.0",
    "squad": "^1.1.3",
    "tildify": "^1.2.0",
    "try-catch": "^1.0.0",
    "writejson": "^1.1.1",
    "yargs": "^4.7.1"
  }
}
